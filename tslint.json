{
  "extends": [
    "tslint:recommended",
    "tslint-config-standard",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rulesDirectory": [],
  "rules": {
    "max-line-length": {
      "options": [120]
    },
    "no-console": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "member-ordering": false
  },
  "jsRules": {
    "max-line-length": {
      "options": [120]
    }
  }
}
